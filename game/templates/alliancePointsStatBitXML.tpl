<entry rank="{@$rank}" allianceID="{@$entry.allianceID}">
	<change>{@$entry.change}</change>
	<tag><![CDATA[{@$entry.allianceTag}]]></tag>
	<name><![CDATA[{@$entry.allianceName}]]></name>
	<points>{@$entry.points}</points>
	<membersCount>{@$entry.membersCount}</membersCount>
	<average>{@$entry.average}</average>
</entry>